{
  "manifest_version": 2,
  "name": "Coins.ph BTC/ETH/XRP Tracker",
  "version": "0.4",
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png"
  }
}